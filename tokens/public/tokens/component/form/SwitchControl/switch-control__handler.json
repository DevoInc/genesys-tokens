{
  "cmp": {
    "switchControl": {
      "handler": {
        "elevation": {
          "boxShadow": {
            "enabled": {
              "value": "{alias.handlers.elevation.boxShadow.enabled.value}"
            },
            "hovered": {
              "value": "{alias.handlers.elevation.boxShadow.hovered.value}"
            },
            "pressed": {
              "value": "{alias.handlers.elevation.boxShadow.pressed.value}"
            }
          }
        },
        "size": {
          "square": {
            "sm": {
              "value": "{alias.handlers.size.square.sm.value}"
            },
            "md": {
              "value": "{alias.handlers.size.square.md.value}"
            },
            "lg": {
              "value": "{alias.handlers.size.square.lg.value}"
            }
          }
        },
        "shape": {
          "borderRadius": {
            "value": "{alias.handlers.shape.borderRadius.value}"
          }
        },
        "mutation": {
          "transitionDuration": {
            "value": "{alias.handlers.mutation.transitionDuration.value}"
          }
        },
        "color": {
          "background": {
            "value": "{alias.handlers.color.background.value}"
          },
          "text": {
            "base": {
              "value": "{alias.handlers.color.text.base.value}"
            },
            "error": {
              "value": "{alias.handlers.color.text.error.value}"
            },
            "warning": {
              "value": "{alias.handlers.color.text.warning.value}"
            },
            "success": {
              "value": "{alias.handlers.color.text.success.value}"
            }
          }
        }
      }
    }
  }
}
