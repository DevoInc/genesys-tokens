            },
            "checked": {
              "base": {
                "value": "{alias.fields.color.background.base.checked.value}"
              },
              "error": {
                "value": "{alias.fields.color.background.error.checked.value}"
              },
              "success": {
                "value": "{alias.fields.color.background.success.checked.value}"
              },
              "warning": {
                "value": "{alias.fields.color.background.warning.checked.value}"
              }
